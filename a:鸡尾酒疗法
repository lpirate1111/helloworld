#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main(){
    int n;
    cin >> n;
    double cocktail, a[n-1];
    int x, y;
   
    
    cin >> x >> y;
    cocktail = y * 1.0 / x;
    
    for (int i = 0; i < n - 1; i++) {
        cin >> x >> y;
        a[i] = y * 1.0 / x;
    }
    
    for (int i = 0; i < n - 1; i++) {
        if (a[i] - cocktail > 0.05) {
            cout << "better" << endl;
        }else if(cocktail - a[i] > 0.05){
            cout << "worse" << endl;
        }else{
            cout << "same" << endl;
        }
    }
    
    return 0;
}

#include <iostream>
using namespace std;

int main(int argc, const char * argv[]) {
    // insert code here...
    char a;
    cin >> a;
    
    
    cout << "  " << a << endl;
    cout << ' ' << a << a << a << endl;
    cout << a << a << a << a << a << endl;
    cout << ' ' << a << a << a << endl;
    cout << "  " << a << endl;

    return 0;
}
